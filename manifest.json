{
  "manifest_version": 2,

  "name": "Not'd",
  "description": "Write notes on web pages",
  "version": "1.0",
  "permissions": [
    "storage",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Not'd",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}
